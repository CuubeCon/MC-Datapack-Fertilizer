#Fuel
scoreboard players add @s cc_fertil_count 1
execute as @s store result score @s cc_fertilizer2 if score @s cc_fertil_count matches 9.. if data block ~ ~ ~ Items[{id:"minecraft:coal",Slot:26b}] run data get block ~ ~ ~ Items[{Slot:26b}].Count
execute if score @s cc_fertil_count matches 9.. if score @s cc_fertilizer2 matches 1.. run function fertilizer:decrease_fuel
execute if score @s cc_fertil_count matches 9.. run scoreboard players reset @s cc_fertil_count
